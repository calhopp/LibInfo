<div class="col-md-6 col-md-offset-3">
	<%= form_for(@lunch) do |f| %>
		<%= f.label :content %>
		<%= f.text_area :content, class: 'form-control' %>

		<%= f.label :date %>
		<%= f.date_field :date, class: 'form-control' %>

		<%= f.submit "Save Lunch", class: "btn btn-primary" %>
	<% end %>
</div>
